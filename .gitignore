priv/static/assets/app.css
priv/static/assets/app.css.map
priv/static/assets/app.js
priv/static/assets/app.js.map
priv/static/assets/vendor.js
priv/static/assets/vendor.js.map

# The directory NPM downloads your dependencies sources to.
/assets/node_modules/

/releases

# We use npm
/assets/yarn.lock
